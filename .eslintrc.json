{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "object-curly-spacing": [2, "always"],
        "linebreak-style": ["error", "windows"],
        "no-unused-vars": "warn",
        "new-cap":"off"
    }
}
